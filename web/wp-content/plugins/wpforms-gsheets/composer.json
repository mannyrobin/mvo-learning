{
	"name": "hipdevteam/wpforms-gsheets",
	"description": "Add form submissions to a Google Sheets spreadsheet",
	"type": "wordpress-plugin",
	"require": {
		"google/apiclient": "^2.1",
		"composer/installers": "^1.0",
		"php": ">=5.6"
	},
	"require-dev": {
		"lucatume/wp-browser": "^1.21",
		"site5/phantoman": "^2.0",
		"jakoch/phantomjs-installer": "2.1.1-p09"
	},
	"scripts": {
		"post-install-cmd": [
			"PhantomInstaller\\Installer::installPhantomJS"
		],
		"post-update-cmd": [
			"PhantomInstaller\\Installer::installPhantomJS"
		]
	},
	"autoload": {
		"psr-4": { "WPFGS\\": "src" }
	}
}
